{
    /*
    	 // Place your snippets for JavaScript here. Each snippet is defined under a snippet name and has a prefix, body and 
    	 // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
    	 // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
    	 // same ids are connected.
    	 // Example:
    	 "Print to console": {
    		"prefix": "log",
    		"body": [
    			"console.log('$1');",
    			"$2"
    		],
    		"description": "Log output to console"
    	}
    */
    "Console Log": {
        "prefix": "log",
        "body": [
            "console.log($1);"
        ],
        "description": "Console log"
    },
    "Console Warn": {
        "prefix": "warn",
        "body": [
            "console.warn($1);"
        ],
        "description": "Console warning"
    },
    "Console Error": {
        "prefix": "error",
        "body": [
            "console.error($1);"
        ],
        "description": "Console error"
    },
    "Console Info": {
        "prefix": "info",
        "body": [
            "console.info($1);"
        ],
        "description": "Console info"
    },
    "Named Function": {
        "prefix": "fn",
        "body": [
            "function $1($2) {",
            "    $3",
            "}"
        ],
        "description": "Create new named function"
    },
    "Un-named Function": {
        "prefix": "ufn",
        "body": [
            "function ($1) {",
            "    $2",
            "}"
        ],
        "description": "Create new unnamed function"
    },
    "Create jQuery wrapper": {
        "prefix": "jqwrap",
        "body": [
            "jqWrap = ${1:$(this)};"
        ],
        "description": "Create jqWrap for default this context"
    },
    "Ajax call": {
        "prefix": "ajax",
        "body": [
            "$.ajax({",
            "   url: '$1',",
            "   method: '${2:get}',",
            "})",
            ".then(${3:successCallback})",
            ".fail(${4:failureCallback})"
        ],
        "description": "Ajax request"
    },
    "Ajax call without callbacks": {
        "prefix": "ajaxwocb",
        "body": [
            "$.ajax({",
            "   url: '${1:requestUrl}',",
            "   method: '${2:get}',",
            "})"
        ],
        "description": "Ajax request without callbacks"
    },
    "Filter Array": {
        "prefix": "filter",
        "body": [
            "${1:array}.filter(${2:filterFunction})"
        ],
        "description": "Filter from array"
    },
    "Map Array": {
        "prefix": "map",
        "body": [
            "${1:array}.map(${2:mapFunction})"
        ],
        "description": "Map array of elements"
    }
}
